<body>
    <section class="ezy__signin15_11JdyjTS d-flex align-items-center justify-content-center vh-100">
        <div class="container">
            <div class="row justify-content-center text-center">
                <div class="col-12 col-md-6 col-lg-5">
                    <div>
                        <h2 class="ezy__signin15_11JdyjTS-heading mb-5">ACCOUNT LOGIN</h2>
                        <form action="/auth/signin" method="POST">
                            <div class="ezy__signin15_11JdyjTS-form-card">
                                <div class="d-flex w-100">
                                    <div class="form-group w-50">
                                        <input type="text" class="form-control" id="name" placeholder="Username"
                                            name="name" />
                                    </div>
                                    <div class="form-group w-50">
                                        <input type="password" class="form-control" id="password" placeholder="Password"
                                            name="password" />
                                    </div>
                                </div>

                                <button type="submit" class="btn ezy__signin15_11JdyjTS-btn w-100">SIGN IN</button>
                            </div>

                            <div class="text-center mt-4">
                                <a href="/auth/forgotPassword">Forget Password ?</a>
                            </div>
                            <div class="text-center mt-4">
                                <a href="/auth/signup">New User? - Sign up - </a>
                            </div>
                            <div class="text-center mt-4">
                                <a href="/">Home</a>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <% if (locals.Message) { %>

            <div class="alert alert-warning alert-dismissible fade show mt-4" role="alert">
                <h4 class="alert-heading">Please Try Again!</h4>
                <hr>
                <%= Message %><strong><%= Username %></strong>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>

            <% } %>
        </div>
    </section>


    <!-- END: Body Content -->

